{
  "functions": {
    "api/*.php": {
      "runtime": "vercel-php@0.6.0"
    },
    "*.php": {
      "runtime": "vercel-php@0.6.0"
    }
  },
  "routes": [
    { "src": "/api/(.*)", "dest": "/api/$1" },
    { "src": "/(.*)\\.php$", "dest": "/$1.php" },
    { "src": "/(.*)", "dest": "/index.php" }
  ],
  "buildCommand": "php vercel_init.php"
}